{
  "name": "aisa",
  "version": "1.0.0",
  "description": "A Software 3D Engine written in TypeScript",
  "scripts": {
    "build": "webpack-cli --mode production",
    "serve": "webpack-dev-server --no-inline --open",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "karma": "karma start",
    "karma-travis-ci": "karma start --browsers=ChromeHeadlessNoSandbox --singleRun=true",
    "obj2json": "ts-node src/blender/obj2json.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdiemke/aisa.git"
  },
  "keywords": [
    "Software Rendering",
    "3D Computer Graphics",
    "TypeScript",
    "Canvas",
    "HTML5"
  ],
  "author": "Johannes Diemke",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "3.3.0",
    "@types/node": "10.12.9",
    "babel-minify-webpack-plugin": "0.3.1",
    "file-loader": "2.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jasmine": "3.3.0",
    "karma": "3.1.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "3.0.5",
    "ts-loader": "5.3.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.6",
    "webpack": "4.26.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  }
}
