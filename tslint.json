{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "array-type": [
            true,
            "generic"
        ],
        "linebreak-style": [
            true,
            "LF"
        ],
        "typedef": [
            false,
            "call-signature",
            "arrow-call-signature",
            "parameter",
            "arrow-parameter",
            "property-declaration",
            "variable-declaration",
            "member-variable-declaration",
            "object-destructuring",
            "array-destructuring"
        ],
        "no-bitwise": false,
        "trailing-comma": false,
        "no-console": false,
        "prefer-for-of": false,
        "no-empty": false,
        "max-line-length": false
    }
}